{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/run-cron-auto-return-bookings",
      "schedule": "0 0 * * *"
    }
  ]
}
